tool outline-tool is {}

process Outliner is
let
  Outliner: outline-tool,
  ParseTree: term,
  FactsStore: term
in
  rec-connect(Outliner?) .
  (
    rec-request(Outliner, processParseTree(ParseTree?)) .
    snd-msg(gs-tree-to-relation(ParseTree, false, false, false)) .
    rec-msg(gs-relation(FactsStore?)) .
    snd-response(Outliner, processParseTree(FactsStore))
  ) * delta
endlet

toolbus(Outliner)